doctype html

link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css'
    integrity='sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u' crossorigin='anonymous')
include header.pug
.body
  .container-50
    .jumbotron
      h1.display-4 Live data
        table.table
          thead
            tr
              th(scope='col') KMPH
              th(scope='col') RPM
              th(scope='col') Coolant Temp
              th(scope='col') Fuel Level
              th(scope='col') MAF
              th(scope='col') Battery Voltage
          tbody
            tr
              if (car_data)
                - console.log("The brackets and quotes "+car_data["RPM"])
                each a in car_data
                  td= a

include footer.pug
